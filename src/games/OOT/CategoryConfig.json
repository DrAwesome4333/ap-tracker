    "options": {
        "er": {
            "title": "Entrance Randomization",
            "type": "category",
            "children": {
                "overworld": {
                    "title": "Shuffle Overworld",
                    "type": "boolean",
                    "default": true
                },
                "interior": {
                    "title": "Shuffle Interiors",
                    "type": "boolean",
                    "default": true
                },
                "dungeons": {
                    "title": "Shuffle Dungeons",
                    "type": "boolean",
                    "default": true
                },
                "boss": {
                    "title": "Shuffle Bosses",
                    "type": "boolean",
                    "default": true
                },
                "grotto": {
                    "title": "Shuffle Grottos & Graves",
                    "type": "boolean",
                    "default": true
                }
            }
        },
        "grouping": {
            "title": "Group Checks For:",
            "type": "category",
            "children": {
                "grottos": {
                    "title": "Grottos and Graves",
                    "type": "boolean",
                    "deafult": true
                },
                "interiors": {
                    "title": "Interiors",
                    "type": "boolean",
                    "deafult": true
                },
                "dungeons": {
                    "title": "Dungeons",
                    "type": "boolean",
                    "deafult": true
                },
                "bosses": {
                    "title": "Dungeon Bosses",
                    "type": "boolean",
                    "deafult": true
                },
                "overworld": {
                    "title": "Overworld Areas",
                    "type": "boolean",
                    "default": true
                },
                "regions": {
                    "title": "Overworld Regions",
                    "type": "boolean",
                    "default": true
                }
            }
        }
    },
    "types": {
        "overworld_region": {
            "show_when": true,
            "type": "standard",
            "flatten_when": { "option": "option.grouping.regions", "is": true }
        },
        "overworld": {
            "type": "standard",
            "show_when": true,
            "flatten_when": {
                "option": "option.grouping.overworld",
                "is": true
            }
        },
        "dungeon": {
            "type": "standard",
            "show_when": true,
            "show_entry_when": { "option": "option.er.dungeon", "is": true }
        },
        "grotto": {
            "type": "portal",
            "show_entry_when": {
                "state": "parent.type",
                "is": "entrance_pool"
            },
            "flatten_when": {
                "option": "option.grouping.grotto",
                "is": false,
                "and": { "option": "option.er.grotto", "is": false }
            },
            "portal_categories": ["Grotto", "Grave"]
        },
        "interior": {
            "type": "portal",
            "show_entry_when": {
                "state": "parent.type",
                "is": "entrance_pool"
            },
            "flatten_when": {
                "option": "grouping.inteiror",
                "is": false,
                "and": { "option": "opiton.er.interior", "is": false }
            },
            "portal_categories": ["Interior", "SpecialInterior"]
        },
        "boss": {
            "type": "portal",
            "show_entry_when": {
                "state": "parent.type",
                "is": "entrance_pool"
            },
            "flatten_when": {
                "option": "option.grouping.boss",
                "is": false,
                "and": { "option": "option.er.boss", "is": false }
            },
            "portal_categories": ["AdultBoss", "ChildBoss"]
        },
        "boss_pool": {
            "type": "entrance_pool",
            "show_when": { "option": "option.er.boss", "is": true },
            "portal_categories": ["boss"]
        },
        "grotto_pool": {
            "type": "entrance_pool",
            "show_when": { "option": "option.er.grotto", "is": true },
            "portal_categories": ["grotto"]
        },
        "interior_pool": {
            "type": "entrance_pool",
            "show_when": { "option": "option.er.interior", "is": true },
            "portal_categories": ["interior"]
        }
    },
    "themes": {
        "forest": { "color": "#009933" },
